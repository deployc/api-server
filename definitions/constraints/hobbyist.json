{
  "kind": "LimitRange",
  "metadata": {
    "name": "usage-limits",
    "labels": {
      "tier": "hobbyist"
    }
  },
  "spec": {
    "limits": [
      {
        "max": {
          "cpu": "500m",
          "memory": "500m"
        },
        "min": {
          "cpu": "200m",
          "memory": "100Mi"
        },
        "type": "Container"
      }
    ]
  }
}
