{
  "kind": "LimitRange",
  "metadata": {
    "name": "usage-limits",
    "labels": {
      "tier": "ultra"
    }
  },
  "spec": {
    "limits": [
      {
        "max": {
          "cpu": "2",
          "memory": "2.5Gi"
        },
        "min": {
          "cpu": "500m",
          "memory": "500Mi"
        },
        "type": "Container"
      }
    ]
  }
}
